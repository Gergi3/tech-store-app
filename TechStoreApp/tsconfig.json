{
	"compileOnSave": true,
	"compilerOptions": {
		"noImplicitAny": false,
		"noEmitOnError": false,
		"removeComments": false,
		"sourceMap": true,
		"target": "ES2015",
		"module": "ES6",
		"outDir": "wwwroot/js",
		"types": [ "jquery" ],
		"allowJs": true
	},
	"include": [
		"wwwroot/js/pages/*",
		"wwwroot/js/constants/*",
		"wwwroot/js/components/*"
	],
	"exclude": []
}
